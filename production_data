--
-- Name: bets_choicebet; Type: TABLE; Schema: public; Owner: django; Tablespace: 
--

CREATE TABLE bets_choicebet (
    prim_key integer NOT NULL,
    name character varying(64) NOT NULL,
    description text NOT NULL,
    created timestamp with time zone NOT NULL,
    pub_date date NOT NULL,
    end_bets_date date,
    resolved boolean NOT NULL,
    pot integer NOT NULL,
    end_date date,
    account_id integer NOT NULL,
    owner_id integer NOT NULL,
    winning_choice_id integer,
    CONSTRAINT bets_choicebet_pot_check CHECK ((pot >= 0)),
    CONSTRAINT bets_choicebet_prim_key_check CHECK ((prim_key >= 0))
);


ALTER TABLE bets_choicebet OWNER TO django;

