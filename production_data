-- Name: bets_datebet; Type: TABLE; Schema: public; Owner: django; Tablespace: 
--

CREATE TABLE bets_datebet (
    prim_key integer NOT NULL,
    name character varying(64) NOT NULL,
    description text NOT NULL,
    created timestamp with time zone NOT NULL,
    pub_date date NOT NULL,
    end_bets_date date,
    resolved boolean NOT NULL,
    pot integer NOT NULL,
    time_period_start date,
    time_period_end date,
    winning_date date,
    account_id integer NOT NULL,
    owner_id integer NOT NULL,
    CONSTRAINT bets_datebet_pot_check CHECK ((pot >= 0)),
    CONSTRAINT bets_datebet_prim_key_check CHECK ((prim_key >= 0))
);


ALTER TABLE bets_datebet OWNER TO django;

